package Practice1;

public class Novel {

	private String name;
	private int volume;
	private double rating;
	private String author;
	
	public Novel(String name, int volume, double rating, String author) {
		this.name = name;
		this.volume = volume;
		this.rating = rating;
		this.author = author;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getVolume() {
		return volume;
	}

	public void setVolume(int volume) {
		this.volume = volume;
	}

	public double getRating() {
		return rating;
	}

	public void setRating(double rating) {
		this.rating = rating;
	}

	public String getAuthor() {
		return author;
	}

	public void setAuthor(String author) {
		this.author = author;
	}


}
