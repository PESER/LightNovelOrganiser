package Practice1;

import java.util.ArrayList;

public class Author {

	private String name;
	private ArrayList<String>seriesWritten;
	
	public Author(String name, ArrayList<String> seriesWritten) {
		this.name = name;
		this.seriesWritten = seriesWritten;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public ArrayList<String> getSeriesWritten() {
		return seriesWritten;
	}

	public void setSeriesWritten(ArrayList<String> seriesWritten) {
		this.seriesWritten = seriesWritten;
	}

}
